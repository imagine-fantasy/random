    {
      "id": "schema_text",
      "type": "text",
      "x": 60,
      "y": 945,
      "width": 510,
      "height": 190,
      "text": "CREATE TABLE token_pool (\n  id               BIGSERIAL PRIMARY KEY,\n  internal_token   VARCHAR(50) NOT NULL UNIQUE,\n  external_ref_id  VARCHAR(16) NOT NULL UNIQUE,\n  status           VARCHAR(20) DEFAULT 'AVAILABLE',\n                   -- AVAILABLE | RESERVED | USED\n  reserved_by      VARCHAR(100),  -- ECS task identifier\n  reserved_at      TIMESTAMP,\n  used_at          TIMESTAMP,\n  card_ref         VARCHAR(50),   -- Card ID after creation\n  created_at       TIMESTAMP DEFAULT NOW()\n);\n\nCREATE INDEX idx_status ON token_pool(status);\nCREATE INDEX idx_external ON token_pool(external_ref_id);",
